<?xml version="1.0" ?>
<odoo>
    <!-- Guardian simple tree view -->
    <record model="ir.ui.view" id="view_partner_guardian_tree">
        <field name="name">Guardian tree view</field>
        <field name="model">res.partner</field>
        <field eval="20" name="priority" />

        <field name="arch" type="xml">
            <tree>
                <field name='name' />
                <field name='email' />
                <field name='phone' />
            </tree>
        </field>
    </record>
</odoo>
